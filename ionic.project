{
  "name": "Toc Messenger",
  "app_id": "com.lewisl.toc",
  "documentRoot": "toc",
  "gulpStartupTasks": [
    "bundle-sass",
    "watch"
  ],
  "watchPatterns": [
    "toc/*",
    "toc/app/*",
    "toc/app/components/**/*",
    "toc/app/libraries/**/*",
    "toc/app/services/**/*",
    "toc/app/views/**/*",
    "!toc/*.scss",
    "!toc/app/*.scss",
    "!toc/app/components/**/*.scss",
    "!toc/app/libraries/**/*.scss",
    "!toc/app/views/**/*.scss"
  ],
  "browsers": [
    {
      "platform": "android",
      "browser": "crosswalk",
      "version": "14.43.343.23"
    }
  ]
}