{
  "name": "Toc Messenger",
  "app_id": "",
  "documentRoot": "app",
  "gulpStartupTasks": [
    "build-sass",
    "watch"
  ],
  "watchPatterns": [
    "app/components/**/*",
    "app/libraries/**/*",
