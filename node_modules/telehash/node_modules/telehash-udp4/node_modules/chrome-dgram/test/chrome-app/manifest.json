{
  "manifest_version": 2,

  "name": "Hello World!",
  "description": "My first Chrome App.",
  "version": "0.1.0",

  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },

  "permissions": [
    "*://*/*"
  ],

  "sockets": {
    "udp": {
      "bind": "*",
      "send": "*"
    }
  }

}