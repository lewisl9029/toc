<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <meta http-equiv="content-type" content="text/html; charset=UTF-8">
  <link rel="stylesheet" media="all" href="<%= relativeDir %>doc-style.css" />
  <script src="<%= relativeDir %>doc-filelist.js"></script>
  <script>
    var relativeDir = '<%= relativeDir %>', thisFile = '<%= filename %>', defaultSidebar = <%= sidebar %>;
  </script>
  <script src="<%= relativeDir %>doc-script.js"></script>
</head>
<body>
  <div id="sidebar_wrapper">
    <div id="sidebar_switch">
      <span class="tree">Files</span>
      <span class="headings">Headings</span>
    </div>
    <div id="tree"></div>
    <div id="headings">
<%    for(var i = 0; i < headings.length; i += 1){ %>
      <div class="heading h<%= headings[i].level %>">
        <a href="#<%= headings[i].id %>"><%= headings[i].text %></a>
      </div>
<%    } %>
    </div>
  </div>
  <div id="sidebar-toggle"></div>
  <div id="container">
    <%= content %>
  </div>
</body>
</html>
