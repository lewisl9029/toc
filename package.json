      "driftyco/ionic": "github:driftyco/ionic@1.1.0",
      "moment": "npm:moment@^2.10.6",
      "ng-cordova": "npm:ng-cordova@^0.1.17-alpha",
      "node-forge": "npm:node-forge@^0.6.34",
