          "angular-sanitize": "^1.3.2",
          "angular-ui-router": "^0.2.10"
        }
      },
      "npm:remotestoragejs@0.11.2": {
        "registry": "jspm",
        "main": "release/0.11.2/remotestorage",
        "format": "global",
        "dependencies": {}
      },
      "npm:angular-toastr@1.4.1": {
        "shim": {
          "dist/angular-toastr.tpls": [
            "angular"
          ]
        },
        "dependencies": {
          "angular": "angular/bower-angular"
        }
      },
      "github:angular-ui/ui-router@0.2.15": {
        "shim": {
          "angular-ui-router": [
            "angular"
          ]
        },
        "dependencies": {
          "angular": "jspm:angular@^1.3.2"
        }
      }
    }
