      "core-js": "npm:core-js@^0.9.4",
      "sinon": "npm:sinon@^1.15.4"
    },
    "overrides": {
      "github:driftyco/ionic@1.1.0": {
        "registry": "jspm",
        "format": "global",
        "main": "release/js/ionic-angular",
        "directories": {
          "lib": "."
        },
        "shim": {
          "release/js/ionic-angular": {
            "deps": [
              "./ionic",
              "angular",
              "angular-animate",
              "angular-sanitize",
              "angular-ui-router"
            ]
          }
        },
        "dependencies": {
          "angular": "^1.4.0",
          "angular-animate": "^1.4.0",
          "angular-sanitize": "^1.4.0",
          "angular-ui-router": "^0.2.10"
        }
      },
      "npm:remotestoragejs@0.11.2": {
        "registry": "jspm",
        "main": "release/0.11.2/remotestorage",
        "format": "global",
        "dependencies": {}
      },
      "github:angular-ui/ui-router@0.2.15": {
        "shim": {
          "angular-ui-router": [
            "angular"
          ]
        },
        "dependencies": {
          "angular": "jspm:angular@^1.4.0"
        }
      },
      "npm:ng-cordova@0.1.17-alpha": {
        "shim": {
          "dist/ng-cordova": [
            "angular"
          ]
        },
        "dependencies": {
          "angular": "angular/bower-angular",
          "conventional-changelog": "0.0.11",
          "fs": "0.0.2",
          "q": "^1.1.2"
        }
      }
    }
