  "description": "Toc Messenger",
  "repository": {
    "type": "git",
    "url": "git@github.com:lewisl9029/toc.git"
  },
  "dependencies": {
    "telehash": "^0.3.23"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.1.5",
    "chai": "^1.10.0",
    "cssgrace": "^1.0.0",
    "del": "^1.1.1",
    "gulp": "^3.8.11",
    "gulp-htmlhint": "0.0.9",
    "gulp-jsbeautifier": "0.0.4",
    "gulp-jshint": "^1.9.2",
    "gulp-postcss": "^4.0.3",
    "gulp-sass": "^1.3.2",
    "gulp-scsslint": "0.0.5",
    "gulp-shell": "^0.2.11",
    "gulp-sourcemaps": "^1.3.0",
    "karma": "^0.12.28",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.1.7",
    "karma-jspm": "^1.1.4",
    "karma-mocha": "^0.1.10",
    "karma-webdriver-launcher": "^1.0.1",
    "mocha": "^2.1.0",
    "postcss": "^4.0.3",
    "run-sequence": "^1.0.2",
    "yargs": "^1.3.3"
  },
  "jspm": {
    "directories": {
      "baseURL": "app",
      "dist": "dist",
      "lib": "app",
      "packages": "app/dependencies"
    },
    "dependencies": {
      "angular": "github:angular/bower-angular@^1.3.2",
      "angular-toastr": "npm:angular-toastr@1.3.1",
      "baobab": "npm:baobab@^1.0.2",
      "css": "github:systemjs/plugin-css@^0.1.0",
      "driftyco/ionic": "github:driftyco/ionic@1.0.0",
      "moment": "npm:moment@^2.9.0",
      "node-forge": "npm:node-forge@^0.6.26",
      "plugin-text": "github:systemjs/plugin-text@^0.0.2",
      "qr-encode": "npm:qr-encode@^0.3.0",
      "ramda": "npm:ramda@^0.11.0",
      "remotestoragejs": "npm:remotestoragejs@0.11.2",
      "scut": "npm:scut@^1.1.2",
      "sinon": "npm:sinon@^1.12.2",
      "text": "github:systemjs/plugin-text@^0.0.2"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.1.13",
      "babel-runtime": "npm:babel-runtime@^5.1.13",
      "core-js": "npm:core-js@^0.9.4"
    },
    "overrides": {
      "github:driftyco/ionic@1.0.0": {
        "registry": "jspm",
        "format": "global",
        "main": "release/js/ionic-angular",
        "directories": {
          "lib": "."
        },
        "shim": {
          "release/js/ionic-angular": {
            "deps": [
              "./ionic",
              "angular",
              "angular-animate",
              "angular-sanitize",
              "angular-ui-router"
            ]
          }
        },
        "dependencies": {
          "angular": "^1.3.2",
          "angular-animate": "^1.3.2",
          "angular-sanitize": "^1.3.2",
          "angular-ui-router": "^0.2.10"
        }
      },
      "npm:remotestoragejs@0.11.2": {
        "registry": "jspm",
        "main": "release/0.11.2/remotestorage",
        "format": "global",
        "dependencies": {}
      },
      "npm:angular-toastr@1.3.1": {
        "shim": {
          "dist/angular-toastr.tpls": [
            "angular"
          ]
        },
        "dependencies": {
          "angular": "angular/bower-angular"
        }
      },
      "github:angular-ui/ui-router@0.2.15": {
        "shim": {
          "release/angular-ui-router": [
            "angular"
          ]
        },
        "dependencies": {
          "angular": "jspm:angular@^1.3.2"
        }
      }
    }
  },
  "cordovaPlugins": [
    "com.ionic.keyboard",
    "cordova-plugin-device",
    "cordova-plugin-console",
    "./engine/cordova-crosswalk-engine-c0.7.1",
    "org.apache.cordova.splashscreen",
    "https://github.com/apache/cordova-plugin-whitelist.git"
  ],
  "cordovaPlatforms": [
    {
      "platform": "android",
      "locator": "./engine/cordova-android-c0.6.1/"
    }
  ]
}
